{
  "extends": [
    "eslint-config-edx",
    "plugin:jasmine/recommended"
  ],
  "sourceType": "module",
  "ecmaVersion": "2017",
  "globals": {
    "FB": true,
    "gettext": true,
    "loadFixtures": true
  },
  "plugins": [
    "jasmine"
  ],
  "env": {
    "jasmine": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  }
}
