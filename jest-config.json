{
  "roots": [
    "<rootDir>/credentials/static/components/"
  ],
  "transform": {
    "^.+\\.jsx?$": "babel-jest",
    "^.+\\.svg$": "<rootDir>/svgTransform.js"
  },
  "setupFiles": [
    "./jest_setup/shim.js",
    "./jest_setup/setupTests.js"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "moduleNameMapper": {
    "axios": "axios/dist/node/axios.cjs"
  },
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/jest_setup/"
  ],
  "transformIgnorePatterns": [
    "/node_modules/(?!(@edx/paragon)/).*/"
  ],
  "watchman": false,
  "testEnvironment": "jsdom"
}
